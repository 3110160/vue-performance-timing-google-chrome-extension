{
    "manifest_version": 2,
  
    "name": "Web Performance Timing API",
    "description": "检测网站加载性能",
    "version": "1.0.0",
    "browser_action": {
      "default_icon": "./assets/icon.png",
      "default_popup": "./popup/index.html"
    },
    "permissions": [
      "activeTab","tabs", "http://*/*", "https://*/*","chrome-search://*/*"
    ]
  }